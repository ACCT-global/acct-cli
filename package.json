{
 "name": "acct-cli",
 "description": "ACCT Toolbelt",
 "version": "0.0.1",
 "author": "Luiz Gustavo Freire Gama",
 "bin": {
  "acct": "./bin/run"
 },
 "bugs": "https://github.com/luizgamabh/acct-cli/issues",
 "dependencies": {
  "@oclif/command": "^1",
  "@oclif/config": "^1",
  "@oclif/plugin-help": "^3",
  "child_process": "^1",
  "inquirer": "^7",
  "shelljs": "^0",
  "tslib": "^1"
 },
 "devDependencies": {
  "@commitlint/cli": "^8",
  "@commitlint/config-conventional": "^8",
  "@oclif/dev-cli": "^1",
  "@oclif/test": "^1",
  "@types/chai": "^4",
  "@types/inquirer": "^7",
  "@types/mocha": "^5",
  "@types/node": "^10",
  "@types/shelljs": "^0",
  "chai": "^4",
  "commitizen": "^4",
  "cz-conventional-changelog": "3.3.0",
  "eslint": "^5.13",
  "eslint-config-oclif": "^3.1",
  "eslint-config-oclif-typescript": "^0.1",
  "eslint-config-prettier": "6.11.0",
  "globby": "^10",
  "husky": "4.2.5",
  "lint-staged": "10.2.13",
  "mocha": "^5",
  "nyc": "^14",
  "prettier": "2.1.1",
  "rimraf": "^3.0.2",
  "ts-node": "^8",
  "typescript": "^3.3"
 },
 "scripts": {
  "postpack": "rimraf oclif.manifest.json",
  "posttest": "eslint . --ext .ts --config .eslintrc",
  "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
  "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
  "version": "oclif-dev readme && git add README.md",
  "lint:fix": "eslint . --ext .ts --fix --config .eslintrc",
  "commit": "cz"
 },
 "husky": {
  "hooks": {
   "pre-commit": "lint-staged",
   "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
   "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
  }
 },
 "lint-staged": {
  "**/*": "prettier --write --ignore-unknown"
 },
 "engines": {
  "node": ">=8.0.0"
 },
 "files": [
  "/bin",
  "/lib",
  "/npm-shrinkwrap.json",
  "/oclif.manifest.json"
 ],
 "homepage": "https://github.com/luizgamabh/acct-cli",
 "keywords": [
  "oclif"
 ],
 "license": "MIT",
 "main": "lib/index.js",
 "oclif": {
  "commands": "./lib/commands",
  "bin": "acct",
  "plugins": [
   "@oclif/plugin-help"
  ]
 },
 "repository": "luizgamabh/acct-cli",
 "types": "lib/index.d.ts",
 "config": {
  "commitizen": {
   "path": "./node_modules/cz-conventional-changelog"
  }
 }
}
